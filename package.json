{
  "name": "sm-server",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "predev": "npm run generate:prisma",
    "dev": "cross-env NODE_ENV=development tsnd --no-notify --respawn ./src/index.ts",
    "prebuild": "npm run clean && npm run migrate && npm run generate:prisma",
    "build": "tsc",
    "seed": "ts-node prisma/seed.ts",
    "clean": "rimraf dist",
    "generate": "npm run generate:prisma && npm run generate:nexus",
    "generate:prisma": "prisma generate",
    "generate:nexus": "ts-node --transpile-only ./src/schema",
    "start": "cross-env NODE_ENV=production node dist/src/index.js",
    "migrate": "prisma migrate deploy ",
    "up": "prisma migrate up --experimental",
    "test": "artillery run artillery.yml",
    "migrate:dev": "prisma migrate dev --preview-feature"
  },
  "dependencies": {
    "@prisma/client": "2.19.0",
    "@tensorflow/tfjs-node": "^3.14.0",
    "bcrypt": "5.0.1",
    "body-parser": "1.19.0",
    "cors": "2.8.5",
    "cross-env": "7.0.3",
    "dotenv": "8.2.0",
    "express": "4.17.1",
    "formidable": "^2.0.1",
    "fs": "^0.0.1-security",
    "helmet": "4.4.1",
    "http-status": "1.5.0",
    "joi": "17.4.0",
    "jsonwebtoken": "8.5.1",
    "multer": "^1.4.4",
    "node-fetch": "^2.6.7",
    "passport": "0.4.1",
    "passport-jwt": "4.0.0",
    "sharp": "^0.30.2",
    "ts-node": "^9.1.1",
    "ts-node-dev": "1.1.6",
    "typescript": "4.2.3"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/cors": "2.8.10",
    "@types/express": "4.17.11",
    "@types/formidable": "^2.0.4",
    "@types/multer": "^1.4.7",
    "@types/node-fetch": "^2.6.1",
    "@types/passport-jwt": "3.0.5",
    "@types/xml2js": "^0.4.9",
    "prisma": "2.19.0",
    "prisma-docs-generator": "0.3.0"
  }
}
